      <th>COLOR</th>
      <th>PRECIO</th>
      <th>STOCK</th>
      <th>ESTADO</th>
      <th>OPCIONES</th>
    </tr>
  </thead>
    <tbody>
        <c:forEach var="var" items="${requestScope.lista}">
